<h1>Sessions#new</h1>
<p>Find me in app/views/sessions/new.html.erb</p>

<%= form_for(:session, url: {action: "create"}, class: "create_form") do |f| %>
	<%= f.label :email %>
	<%= f.email_field :email %><br>

	<%= f.label :password %>
	<%= f.password_field :password %><br>	

	<%= f.submit "Create "%>
<% end %>

<%= link_to "signout", signout_path, method: :delete %>